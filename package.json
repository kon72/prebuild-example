{
  "name": "prebuild-example",
  "private": true,
  "author": "kon72 <kinsei0916@gmail.com>",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "node-api-headers": "^1.0.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3"
  },
  "pnpm": {
    "$comment": "See https://github.com/prebuild/prebuild/issues/286",
    "overrides": {
      "node-gyp": "^9.4.0"
    }
  },
  "packageManager": "pnpm@8.6.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kon72/prebuild-example.git"
  }
}
